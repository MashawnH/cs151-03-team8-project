<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddController">
   <children>
      <HBox prefHeight="78.0" prefWidth="400.0">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
         <children>
            <Label prefHeight="42.0" prefWidth="86.0" text="Date: ">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Label>
            <DatePicker prefHeight="42.0" prefWidth="155.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></DatePicker>
         </children>
      </HBox>
      <HBox prefHeight="78.0" prefWidth="400.0">
         <children>
            <Label prefHeight="42.0" prefWidth="86.0" text="Label: ">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <CheckBox mnemonicParsing="false" prefHeight="42.0" prefWidth="129.0" text="Expense">
               <font>
                  <Font size="18.0" />
               </font>
            </CheckBox>
            <CheckBox mnemonicParsing="false" prefHeight="42.0" prefWidth="131.0" text="Income">
               <font>
                  <Font size="18.0" />
               </font>
            </CheckBox>
         </children>
      </HBox>
      <HBox prefHeight="78.0" prefWidth="400.0">
         <children>
            <Label prefHeight="42.0" prefWidth="86.0" text="Category:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Label>
            <ChoiceBox prefHeight="42.0" prefWidth="140.0">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox prefHeight="78.0" prefWidth="400.0">
         <children>
            <Label prefHeight="42.0" prefWidth="86.0" text="Amount: ">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Label>
            <TextField prefHeight="42.0" prefWidth="183.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="goBack" mnemonicParsing="false" onAction="#goBack" prefHeight="48.0" prefWidth="101.0" text="Go Back">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="add" mnemonicParsing="false" onAction="#add" prefHeight="48.0" prefWidth="101.0" text="Add">
               <HBox.margin>
                  <Insets left="110.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
